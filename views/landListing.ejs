<!DOCTYPE html>
<html lang="en">

<head>
    <%- include ('./partials/head') %>
</head>

<body>

    <div id="preloader">
        <div class="south-load"></div>
    </div>

    <header>
        <%- include('./partials/header') %>
    </header>


    <section class="breadcumb-area bg-img" style="background-image: url(/images/landbg.jpeg);">
        <div class="container h-100">
            <div class="row h-100 align-items-center">
                <div class="col-12">
                    <div class="breadcumb-content">
                        <h3 class="breadcumb-title">Land Listings</h3>
                    </div>
                </div>
            </div>
        </div>
    </section>


    <section class="listings-content-wrapper section-padding-100">
        <div class="container">
            
            
            <div class="row">
                <% for(var x=0; x<docs.length; x++) {%>

                      
                <div class="col-12 col-md-6 col-xl-4">
                    <div class="single-featured-property mb-50 wow fadeInUp" data-wow-delay="100ms">

                        <div class="property-thumb">
                            <a href="#" ><img style="height:300px; width:100rem" src="/<%= docs[x].image %>" alt=""></a>
                            <div class="tag">
                                <span>For Sale</span>
                            </div>
                            <div class="list-price">
                                <p>

                                    <%= docs[x].price %>
                                    <span><%docs[x].fee %></span>
                                </p>
                            </div>
                        </div>

                        <div class="property-content">
                            <h5>
                                <%= docs[x].projectName%>
                            </h5>
                            <p class="location" style="text-align:center!important;">
                                <img src="img/icons/location.png" alt="">
                              Address:  <%= docs[x].address %>, <span> <%= docs[x].city %></span>, <span> <%= docs[x].state %></span>
                            </p>
                            <p>
                                <%= docs[x].description %>
                            </p>
                             
                            <div>

                            </div>
                                                   
                        </div>
                    </div>
                </div>
                <% } %>

            </div>
            <div class="row">
                <div class="col-12">
                    <div class="south-pagination d-flex justify-content-end">
                        <nav aria-label="Page navigation">
                            <ul class="pagination">
                                <li class="page-item"><a class="page-link active" href="#">01</a></li>
                                <li class="page-item"><a class="page-link" href="#">02</a></li>
                                <li class="page-item"><a class="page-link" href="#">03</a></li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </section>


    <footer>

        <%- include('./partials/footer') %>
    </footer>
    <script>var coll = document.getElementsByClassName("collapsible");
        var i;
        
        for (i = 0; i < coll.length; i++) {
          coll[i].addEventListener("click", function() {
            this.classList.toggle("active");
            var content = this.nextElementSibling;
            if (content.style.maxHeight){
              content.style.maxHeight = null;
            } else {
              content.style.maxHeight = content.scrollHeight + "px";
            } 
          });
        }</script>
    <%- include('./partials/scripts') %>
</body>

</html>