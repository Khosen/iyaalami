<!doctype html>
<html lang="en">

<head>

    <%- include('./partials/dashboardhead') %>
        <link href="/css/form.css" rel="stylesheet">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">


</head>

<body class="text-center container">
    <%- include('./partials/dashboardnav') %>

        <!--  <div class="col-md-8 col-lg-12 text-center">-->

        <div class="form-row col-md-8 col-lg-12 text-center">

            <form class="form-fill" action="/admin/propertyType" method="post">
                <h5 style="font-weight: bold; margin-top: 5rem;">Iya-Alami Estate Property Type</h5>
                <span style="font-size: x-small;">please fill in the details of property type you offer to clients</span>
                <br>
                <br>
                <span> <% if (locals.message.length>0) { %>
                    <ul>
                      
                            <li class="alert alert-danger" style="list-style:none; font-weight: bold;">
                                <%= message%>
                            </li>
                       
                    </ul>
                    <% } %>
                <h6 style="font-weight: bold;">
                 
                </h6>
                <div class="form-row">
                    <div class="input-field col s2">
                        <input placeholder="eg.: Iya-Alami Iconic Homes" id="type" name="type" type="text" class="validate">
                        <label for="type" class="fontsize">property Type</label>
                    </div>
                  
                </div>
               

               
                <button class="btn btn-primary blue" type="submit" style="color:white;">Submit form</button>


            </form>

        </div>



        <!-- /.container -->
        <%- include('./partials/dashboardscript') %>
            <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
            <!-- Compiled and minified JavaScript -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
            <script>
                $(document).ready(function() {
                    $('select').formSelect();
                });
            </script>
            <script>
                $('#situation').val('');
                M.textareaAutoResize($('#situation'));
            </script>
            <script>
                $('#features').val('');
                M.textareaAutoResize($('#features'));
            </script>

</body>

</html>