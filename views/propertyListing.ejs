    <% for(var x=0; x<docs.length; x++) {%>

<div class="row">
  
<div class="col-12 col-lg-8">
  
             
<div class="single-blog-area mb-50">

<div class="blog-post-thumbnail">
<img  src="/<%= docs[x].image %>" alt="">
</div>

</div>

</div>
